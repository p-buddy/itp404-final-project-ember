<nav class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
      <ul class="navbar-brand">Puddy</ul>
    </div>
    <ul class="nav navbar-nav" class="navbar" class="menu">
      <li>{{#link-to "login" }}Log Out{{/link-to}}</li>
      <li>{{#link-to "discover" model.id}}Discover{{/link-to}}</li>
      <li>{{#link-to "profile" model.id}}History{{/link-to}}</li>
