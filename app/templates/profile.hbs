{{outlet}}


<table id="table" class="table table-striped table-bordered table-condensed table-hover" >
       <tr>
           <th>
               Artist
           </th>
           <th>
               Link
           </th>
           <th>
               Delete
           </th>
       </tr>

{{#each model.data as |artist|}}
       <tr id={{artist.id}} class="history">
           <td>
               {{artist.artists}}
           </td>
            <td>
              <a href={{artist.link}}>Youtube</a>
            </td>
            <td>
              <button class="toFloat btn-danger btn-lg" type="button" {{action 'Delete' artist.id}}> Delete
              </button>
            </td>
       </tr>
{{/each}}

   </table>
